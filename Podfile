# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'SecretMessenger' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SecretMessenger
  # Model libraries
  pod 'ObjectMapper'

  # UI libraries
  pod 'AlamofireImage'
  pod 'JLToast'
  pod 'KRProgressHUD'
  pod 'DGElasticPullToRefresh_CanStartLoading'

  # Social libraries
  pod 'Firebase'
  pod 'FirebaseAuth'
  pod 'FirebaseDatabase'
  pod 'FBSDKCoreKit'
  pod 'FBSDKLoginKit'
  pod 'GoogleSignIn'

  # Cryptography
  pod 'CryptoSwift'

end
